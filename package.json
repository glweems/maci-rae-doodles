{
  "name": "maci-rae-doodles",
  "version": "0.0.1",
  "description": "Website for Maci Rae Doodles dog breeding and training",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -o ./app/tailwind.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -o ./app/tailwind.css --watch",
    "postinstall": "remix setup node",
    "start": "remix-serve build",
    "build:remix": "remix build",
    "dev:remix": "node -r dotenv/config node_modules/.bin/remix dev"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.7.4",
    "@cloudinary/react": "^1.0.1",
    "@cloudinary/url-gen": "^1.4.2",
    "@craco/craco": "^6.4.3",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@headlessui/react": "^1.4.3",
    "@heroicons/react": "^1.0.5",
    "@remix-run/react": "^1.1.1",
    "@remix-run/serve": "^1.1.1",
    "@remix-run/vercel": "^1.1.1",
    "@supabase/supabase-js": "^1.29.1",
    "@supabase/ui": "^0.36.2",
    "@vanilla-extract/css": "^1.6.8",
    "assert": "^2.0.0",
    "dotenv": "^12.0.3",
    "framer-motion": "^5.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.1",
    "ts-dotenv": "^0.8.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.1",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "concurrently": "^7.0.0",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.15",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/glweems/maci-rae-doodles.git"
  },
  "author": "Garrett W <gwgraphicdesign@gmail.com> (https://glweems.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/glweems/maci-rae-doodles/issues"
  },
  "homepage": "https://github.com/glweems/maci-rae-doodles#readme"
}
