{
  "name": "maci-rae-doodles",
  "version": "0.0.1",
  "description": "Website for Maci Rae Doodles dog breeding and training",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -o ./app/tailwind.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -o ./app/tailwind.css --watch",
    "start": "remix-serve build",
    "build:remix": "remix build",
    "dev:remix": "remix dev",
    "lint": "eslint \"app/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"app/**/*.{ts,tsx}\" --fix",
    "prepare": "husky install",
    "commit": "git-cz"
  },
  "husky": {
    "hooks": {
      "pre-commit": [
        "lint-staged"
      ],
      "prepare-commit-msg": "/usr/local/bin/git cz --hook || true",
      "pre-push": [
        "npm run build",
        "git add -A",
        "lint-staged"
      ]
    }
  },
  "lint-staged": {
    "app/**/.{ts,tsx}": [
      "./node_modules/.bin/eslint --fix",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/prompt"
    }
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.1.7",
    "@chakra-ui/react": "^1.8.8",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@headlessui/react": "^1.5.0",
    "@heroicons/react": "^1.0.6",
    "@mantine/core": "^4.1.3",
    "@mantine/hooks": "^4.1.3",
    "@mantine/prism": "^4.1.3",
    "@mantine/rte": "^4.1.3",
    "@mantine/ssr": "^4.1.3",
    "@next-boost/hybrid-disk-cache": "^0.3.0",
    "@remix-run/node": "^1.4.0",
    "@remix-run/react": "^1.4.0",
    "@remix-run/serve": "^1.4.0",
    "@remix-run/vercel": "^1.4.0",
    "@vanilla-extract/css": "^1.7.0",
    "airtable": "^0.11.3",
    "dotenv": "^16.0.0",
    "eslint-plugin-import": "^2.26.0",
    "framer-motion": "^6.3.0",
    "isbot": "^3.4.5",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-syntax-highlighter": "^15.5.0",
    "react-table": "^7.7.0",
    "react-use": "^17.3.2",
    "remix": "^1.4.0",
    "remix-image": "^0.3.16",
    "sharp": "^0.30.3",
    "ts-dotenv": "^0.8.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@commitlint/prompt": "^16.2.3",
    "@remix-run/dev": "^1.4.0",
    "@types/node": "^17.0.24",
    "@types/prettier": "^2.6.0",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "@types/react-syntax-highlighter": "^13.5.2",
    "@types/react-table": "^7.7.10",
    "@types/sharp": "^0.30.2",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "commitizen": "^4.2.4",
    "concurrently": "^7.1.0",
    "conventional-changelog-atom": "^2.0.8",
    "eslint": "^8.13.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-strict-booleans": "^1.0.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "postcss": "^8.4.12",
    "postcss-import": "^14.1.0",
    "prettier": "^2.6.2",
    "prettier-eslint": "^13.0.0",
    "react-json-view": "^1.21.3",
    "tailwindcss": "^3.0.24",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/glweems/maci-rae-doodles.git"
  },
  "author": "Garrett W <gwgraphicdesign@gmail.com> (https://glweems.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/glweems/maci-rae-doodles/issues"
  },
  "homepage": "https://github.com/glweems/maci-rae-doodles#readme",
  "prisma": {
    "seed": "node --loader ts-node/esm prisma/seed.ts"
  }
}
