{
  "name": "maci-rae-doodles",
  "version": "0.0.1",
  "description": "High quality family Bernedoodle puppies",
  "scripts": {
    "postinstall": "remix setup node",
    "start": "remix-serve build",
    "build": "remix build",
    "dev": "remix dev",
    "lint": "eslint \"app/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"app/**/*.{ts,tsx}\" --fix",
    "prepare": "husky install",
    "commit": "git-cz",
    "clean": "rm -rf .cache && echo 'Cache cleaned' && remix build && echo 'App built'",
    "scrape": "ts-node bin/scrapeEmbark.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": [
        "lint-staged"
      ],
      "pre-push": [
        "npm run build",
        "git add -A",
        "lint-staged"
      ]
    }
  },
  "lint-staged": {
    "app/**/.{ts,tsx}": [
      "./node_modules/.bin/eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.1.5",
    "@chakra-ui/react": "^1.8.3",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mantine/ssr": "^3.6.9",
    "airtable": "^0.11.2",
    "dotenv": "^16.0.0",
    "eslint-plugin-import": "^2.25.4",
    "framer-motion": "^6.2.6",
    "fs-extra": "^10.0.0",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.1",
    "pill-pity": "^0.2.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-ga": "^3.3.0",
    "react-icons": "^4.3.1",
    "react-iframe": "^1.8.0",
    "react-slick": "^0.28.1",
    "react-syntax-highlighter": "^15.4.5",
    "react-table": "^7.7.0",
    "react-use": "^17.3.2",
    "remix": "^1.2.1",
    "ts-dotenv": "^0.8.3"
  },
  "devDependencies": {
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-slick": "^0.23.8",
    "@types/react-syntax-highlighter": "^13.5.2",
    "@types/react-table": "^7.7.9",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "csstype": "^3.0.10",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "react-json-view": "^1.21.3",
    "ts-node": "^10.5.0",
    "typescript": "^4.7.0-dev.20220218"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/glweems/maci-rae-doodles.git"
  },
  "author": "Garrett W <gwgraphicdesign@gmail.com> (https://glweems.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/glweems/maci-rae-doodles/issues"
  },
  "homepage": "https://github.com/glweems/maci-rae-doodles#readme"
}
