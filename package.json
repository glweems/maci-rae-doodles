{
  "name": "maci-rae-doodles",
  "version": "0.0.1",
  "description": "Website for Maci Rae Doodles dog breeding and training",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -o ./app/tailwind.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -o ./app/tailwind.css --watch",
    "postinstall": "remix setup node",
    "start": "remix-serve build",
    "build:remix": "remix build",
    "dev:remix": "remix dev",
    "lint": "eslint \"app/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"app/**/*.{ts,tsx}\" --fix",
    "prepare": "husky install",
    "commit": "git-cz"
  },
  "husky": {
    "hooks": {
      "pre-commit": [
        "lint-staged"
      ],
      "pre-push": [
        "npm run build",
        "git add -A",
        "lint-staged"
      ]
    }
  },
  "lint-staged": {
    "app/**/.{ts,tsx}": [
      "./node_modules/.bin/eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.1.5",
    "@chakra-ui/react": "^1.8.3",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@headlessui/react": "^1.4.3",
    "@heroicons/react": "^1.0.5",
    "@remix-run/react": "^1.1.3",
    "@remix-run/serve": "^1.1.3",
    "@remix-run/vercel": "^1.1.3",
    "@vanilla-extract/css": "^1.6.8",
    "airtable": "^0.11.1",
    "dotenv": "^16.0.0",
    "eslint-plugin-import": "^2.25.4",
    "framer-motion": "^6.2.4",
    "lodash": "^4.17.21",
    "nanoid": "^3.2.0",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-syntax-highlighter": "^15.4.5",
    "react-table": "^7.7.0",
    "react-use": "^17.3.2",
    "remix": "^1.1.3",
    "ts-dotenv": "^0.8.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@commitlint/prompt": "^14.1.0",
    "@remix-run/dev": "^1.1.3",
    "@types/node": "^17.0.15",
    "@types/prettier": "^2.4.3",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-syntax-highlighter": "^13.5.2",
    "@types/react-table": "^7.7.9",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "commitizen": "^3.0.0",
    "concurrently": "^7.0.0",
    "conventional-changelog-atom": "^2.0.8",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-strict-booleans": "^1.0.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.3",
    "postcss": "^8.4.6",
    "postcss-import": "^14.0.2",
    "prettier": "^2.5.1",
    "prettier-eslint": "^8.2.2",
    "react-json-view": "^1.21.3",
    "tailwindcss": "^3.0.18",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/glweems/maci-rae-doodles.git"
  },
  "author": "Garrett W <gwgraphicdesign@gmail.com> (https://glweems.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/glweems/maci-rae-doodles/issues"
  },
  "homepage": "https://github.com/glweems/maci-rae-doodles#readme",
  "prisma": {
    "seed": "node --loader ts-node/esm prisma/seed.ts"
  }
}
